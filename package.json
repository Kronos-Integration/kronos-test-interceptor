{
  "name": "@kronos-integration/test-interceptor",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/module.mjs",
  "module": "src/module.mjs",
  "description": "test support for kronos interceptors",
  "keywords": [
    "test"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/module.mjs --section=API",
    "lint": "documentation lint src/module.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "kronos-endpoint": "^2.14.15",
    "markdown-doctest": "^1.0.0",
    "semantic-release": "^15.13.29"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/test-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/test-interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/test-interceptor#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
