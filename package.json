{
  "name": "kronos-test-interceptor",
  "version": "0.0.0-semantic-release",
  "description": "test support for kronos interceptors",
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "node_modules/rollup/bin/rollup --output=dist/module.js -c -- src/module.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-test-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-test-interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-test-interceptor#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.2"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.2"
  },
  "dependencies": {
    "kronos-interceptor": "^2.4.1",
    "loglevel-mixin": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": ["test"]
}
